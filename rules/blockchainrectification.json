{
    "name": "blockchainrectification",
    "author": "Daniel Demelash",
    "description": "generic rule for scam site that claim to be Wallet Rectification",
    "target": "",
    "composite_flag_conditions": {
        "match_condition": "any",
        "conditions": [
            {
                "logical_operator": "and",
                "conditions": [
                    {
                        "attribute": "title",
                        "operator": "contains",
                        "value": "Blockchain Rectification"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Blockchain technology makes this new aproch to digital ownership possible."
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "decentralized protocol"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Make Your Selection Below"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Blockchain Rectification . All rights reserved."
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Connect Wallet"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Select Chain"
                    }
                ]
            }
        ]
    },
    "composite_false_positives": {
        "match_condition": "any",
        "conditions": []
    }
}