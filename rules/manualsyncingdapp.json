{
    "name": "manualsyncingdapp",
    "author": "Daniel Demelash",
    "description": "Rule for scam site Dapps, similar to manualsyncingdapp.netlify.app",
    "target": "",
    "composite_flag_conditions": {
        "match_condition": "any",
        "conditions": [
            {
                "logical_operator": "and",
                "conditions": [
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "MANUAL SYNCING DAPPn"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Manual Syncing Dapp  is a decentralized platform and network that blends Blockchain with DeFi"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "<!-- Added by HTTrack -->"
                    },
                    {
                        "attribute": "title",
                        "operator": "contains",
                        "value": "MANUALSYNCINGDAPP - Bitcoin And Crypto Currency"
                    }
                ]
            }
        ]
    },
    "composite_false_positives": {
        "match_condition": "any",
        "conditions": []
    }
}