{
    "name": "v4runvault",
    "author": "Daniel Demelash",
    "description": "Rule for scam site that claim to be Blockchain Rectification app, similar to v4runvault.com",
    "target": "",
    "composite_flag_conditions": {
        "match_condition": "any",
        "conditions": [
            {
                "logical_operator": "and",
                "conditions": [
                    {
                        "attribute": "title",
                        "operator": "contains",
                        "value": "Blockchain Rectification - We are help to help you resolve your crypto related issues"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Blockchain Integration Framework"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Select Chain"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Connect Wallet"
                    },
                    {
                        "attribute": "html",
                        "operator": "contains",
                        "value": "Copyright Â© 2023 Blockchain Rectification . All rights reserved."
                    }
                ]
            }
        ]
    },
    "composite_false_positives": {
        "match_condition": "any",
        "conditions": []
    }
}